{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "npx-idp-schematic": {
      "description": "Schematic to configure IDPs",
      "factory": "../dist/archmatic/npx-idp-schematic/index#idpSchematic",
      "schema": "./npx-idp-schematic/schema.json"
    },
    "npx-global-handler": {
      "description": "Schematic to handle exceptions globally, etc .",
      "factory": "../dist/archmatic/npx-global-handler"
    },
    "npx-add": {
      "description": "A schematic to add basic architecture elements",
      "factory": "../dist/archmatic/npx-add",
      "schema": "./npx-add/schema.json"
    },
    "npx-add-analytics": {
      "description": "Schematic to add Analytics",
      "factory": "../dist/archmatic/npx-add-analytics/index#addAnalytics",
      "schema": "./npx-add-analytics/schema.json"
    },
    "my-extend-schematic": {
      "description": "A schematic that extends another schematic.",
      "extends": "my-full-schematic"
    }
  }
}